<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:LuminoStudio.Views.Themes">

    <!-- DockManager の背景色 -->
    <SolidColorBrush x:Key="AvalonDock_ThemeMetro_BaseColor1" 
                     Color="Transparent"/>

    <!-- ウィンドウタイトルなどの文字色 -->
    <SolidColorBrush x:Key="AvalonDock_ThemeMetro_BaseColor2" 
                     Color="White"/>

    <!-- アクティブなウィンドウのタイトル・タブを修飾するバーの色 -->
    <SolidColorBrush x:Key="AvalonDock_ThemeMetro_BaseColor3" 
                     Color="#41B1E1"/>
    

    <SolidColorBrush x:Key="AvalonDock_ThemeMetro_BaseColor4" 
                     Color="#5EA1C0"/>

    <!-- 非アクティブなウィンドウのタイトル・タブを修飾するバーの色 -->
    <!-- タブのタイトル文字色 -->
    <SolidColorBrush x:Key="AvalonDock_ThemeMetro_BaseColor5" 
                     Color="#CCCCCC"/>

    <!-- フローティング時のウィンドウの枠線 -->
    <SolidColorBrush x:Key="AvalonDock_ThemeMetro_BaseColor6" 
                     Color="#8C8E94"/>

    <!-- ドッキング時の Layout 境界に表示されるライン -->
    <SolidColorBrush x:Key="AvalonDock_ThemeMetro_BaseColor7" 
                     Color="#3F3F3F"/>


    <!-- コンテンツ領域の背景色 -->
    <SolidColorBrush x:Key="AvalonDock_ThemeMetro_WindowBackground" 
                     Color="#252525"/>

    <!-- コンテンツ領域の背景色 -->
    <SolidColorBrush x:Key="AvalonDock_ThemeMetro_ContentBackground" 
                     Color="#1F1F1F"/>

</ResourceDictionary>